{
    "name": "excel-to-json.mlai",
    "version": "0.2.1",
    "description": "Transform any excel file into JSON with support for multiple sheets and online files",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "keywords": [
        "excel",
        "json",
        "xlsx",
        "csv",
        "converter",
        "sheets",
        "mlai"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Mathieu-ai/ExcelToJson.git"
    },
    "scripts": {
        "build": "npm run clean && tsc",
        "clean": "rm -rf dist/",
        "format": "prettier --write \"src/**/*.ts\"",
        "prepublishOnly": "npm run build"
    },
    "author": "mathieu-ai <mathieu.lievre@efrei.net>",
    "license": "MIT",
    "dependencies": {
        "generic-functions.mlai": "^0.8.3",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "typescript": "^5.0.0",
        "prettier": "^3.0.0"
    },
    "engines": {
        "node": ">=14.0.0"
    }
}