{
    "name": "excel-to-json.mlai",
    "version": "0.2.7",
    "description": "Transform Excel files into JSON with advanced features including nested objects, Google Sheets support, and multiple sheet handling",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "keywords": [
        "excel",
        "json",
        "xlsx",
        "csv",
        "converter",
        "sheets",
        "mlai",
        "nested-objects",
        "google-sheets",
        "typescript",
        "parser",
        "spreadsheet"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Mathieu-ai/ExcelToJson.git"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        },
        "./types": {
            "types": "./dist/types.d.ts",
            "import": "./dist/types.js",
            "require": "./dist/types.js"
        }
    },
    "scripts": {
        "build": "tsup",
        "build:watch": "tsup --watch",
        "clean": "node scripts/build-helper.cjs clean",
        "copy-files": "node scripts/build-helper.cjs copy-files",
        "prepare-dist": "npm run build && npm run copy-files",
        "prebuild": "npm run clean",
        "prepare": "npm run prepare-dist",
        "prepack": "npm run prepare-dist",
        "test": "echo \"No tests specified\" && exit 0",
        "dev": "tsup --watch --onSuccess \"node dist/index.js\"",
        "typecheck": "tsc --noEmit"
    },
    "files": [
        "dist"
    ],
    "author": "mathieu-ai <mathieu.lievre@efrei.net>",
    "license": "MIT",
    "dependencies": {
        "generic-functions.mlai": "^0.9.17",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "prettier": "^3.0.0",
        "tsup": "^8.5.0",
        "typescript": "^5.0.0"
    },
    "engines": {
        "node": ">=14.0.0"
    }
}